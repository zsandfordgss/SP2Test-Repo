<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <fujitsu.cobol>
    <runtime>
      <environmentSettings>
        <add key="@CBR_FILE_USE_MESSAGE" value="YES" />
        <add key="@CBR_FILE_SEQUENTIAL_ACCESS" value="BSAM" />
        <add key="@CBR_TRAILING_BLANK_RECORD" value="REMOVE" />
        <add key="@CBR_EXFH_CLASS" value="GSSERP.FileHandler" />
        <add key="@CBR_EXFH_LOAD" value="GSSERP.FujitsuExternalFileHandler, Version=1.0.0.0, Culture=neutral, PublicKeyToken=ca1edae32e83318f" />
        <add key="@CBR_ENTRYFILE" value="FujitsuEntry.config" />
      </environmentSettings>
    </runtime>
  </fujitsu.cobol>
  <runtime>
    <loadFromRemoteSources enabled="true"/>
  </runtime>
</configuration>